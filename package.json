    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-validator": "^6.14.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multiparty": "^4.2.3",
    "nodemailer": "^6.7.7"
  },
  "devDependencies": {
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.18",
    "terser": "^5.14.1"
  }
}
